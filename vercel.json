
{
  "version": 2,
  "buildCommand": "npm run build && chmod +x build-hooks.sh && ./build-hooks.sh",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/Visible Ores.mcpack",
      "destination": "/Visible Ores.mcpack"
    },
    {
      "source": "/api/texture-packs/:id/download",
      "destination": "/api/index.mjs"
    }
  ],
  "functions": {
    "api/index.mjs": {
      "includeFiles": "public/**,attached_assets/**,dist/**"
    }
  }
}
