
{
  "version": 2,
  "buildCommand": "npm run build && chmod +x build-hooks.sh && ./build-hooks.sh",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/Visible Ores.mcpack", "destination": "/api/download" },
    { "source": "/texture-pack", "destination": "/api/texture-pack" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
